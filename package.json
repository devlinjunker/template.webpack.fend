{
  "version": "0.8.0-SNAPSHOT",
  "name": "Template-WebpackFEND",
  "description": "Template using Webpack to create a html/javascript/css pages with multiple files",
  "engines": {
    "node": ">=10.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/devlinjunker/template.webpack.fend"
  },
  "keywords": [
    "template",
    "webpack",
    "fend",
    "babel",
    "flow",
    "eslint",
    "esdoc",
    "browser",
    "html",
    "css",
    "javascript"
  ],
  "author": "devlin.junker@gmail.com",
  "license": "MIT",
  "main": "target/app.bundle.js",
  "scripts": {
    "start-watch": "node_modules/karma/bin/karma start --env.dev_server",
    "dev-watch": "./node_modules/.bin/webpack --env.dev_server",
    "test-watch": "node_modules/karma/bin/karma start",
    "test-dev": "node_modules/karma/bin/karma start --single-run=false --browser=Chrome --auto-watch",
    "test": "node_modules/karma/bin/karma start --single-run",
    "build": "./node_modules/.bin/webpack",
    "lint": "./node_modules/.bin/eslint ./src",
    "doc": "npm run build && (./node_modules/.bin/esdoc; rm -rf docs/demo/; mv -f public/ docs/demo/)",
    "doc-image": "madge ./src -i docs/image/dependency.png && npm run doc",
    "clean": " ## TODO: clean up project files"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "normalize.css": "^8.0.1",
    "tailwindcss": "^1.4.4"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/polyfill": "^7.8.7",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-flow": "^7.9.0",
    "@babel/register": "^7.9.0",
    "autoprefixer": "^9.7.6",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "circular-dependency-plugin": "^5.2.0",
    "clean-webpack-plugin": "^1.0.1",
    "css-loader": "^2.1.0",
    "electron-rebuild": "^1.10.1",
    "esdoc": "^1.1.0",
    "esdoc-flow-plugin": "^1.0.0",
    "esdoc-standard-plugin": "^1.0.0",
    "eslint": "^5.15.1",
    "eslint-config-prettier": "^4.1.0",
    "eslint-import-resolver-webpack": "^0.11.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-flowtype": "^3.4.2",
    "eslint-plugin-flowtype-errors": "^4.2.0",
    "eslint-plugin-header": "^3.0.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-mocha": "^6.3.0",
    "eslint-plugin-prettier": "^3.1.3",
    "file-loader": "^3.0.1",
    "flow-bin": "^0.93.0",
    "flow-typed": "^2.6.2",
    "flow-webpack-plugin": "^1.2.0",
    "handlebars": "^4.7.6",
    "handlebars-loader": "^1.7.1",
    "html-webpack-plugin": "^3.2.0",
    "htmlhint": "^0.11.0",
    "ignore-styles": "^5.0.1",
    "karma": "^3.1.4",
    "karma-chrome-launcher": "^2.2.0",
    "karma-eslint": "^2.2.0",
    "karma-firefox-launcher": "^1.3.0",
    "karma-mocha": "^1.3.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^4.0.2",
    "list-selectors": "^2.0.0",
    "madge": "^4.0.1",
    "mini-css-extract-plugin": "^0.9.0",
    "mocha": "^10.2.0",
    "postcss-exclude-files": "^1.0.15",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "postcss-reporter": "^6.0.1",
    "precss": "^4.0.0",
    "prettier": "^1.19.1",
